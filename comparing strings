#include<stdio.h>
#include<string.h>
void main()
{
    char str1[50],str2[50];
    int a;
    printf("enter a password:");
    fgets(str1,50,stdin);
    printf("reenter the password:");
    fgets(str2,50,stdin);
    a=strcmp(str1,str2);
    if(a==0)
    {
        printf("matched");
    }
    else
    {
        printf("mismatched");
    }
}
