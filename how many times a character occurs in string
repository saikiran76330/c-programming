#include <stdio.h>
#include<string.h>
void main()
{
    char str[50],l;
    int i,a,b,flag=0;
    printf("Enter your name :");
    fgets(str,50,stdin);
    printf("Enter charcter to search:");
    scanf("%c",&l);
    for(i=0;i<strlen(str);i++)
    {
        if(l==str[i])
        {
          flag+=1;
            
        }
       
    }
    if(flag>0)
    {
        printf("Yes");
    }
    else
    {
        printf("No");
    }
    printf("\nit is present %d times",flag);
}
