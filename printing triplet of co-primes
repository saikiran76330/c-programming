#include<stdio.h>
#include<string.h>
#include<stdbool.h>
bool prime(int,int);
int gcd(int,int);
int gcd(int a,int b)
{
    int temp=0,j;
    while(b>0)
    {
        temp=a;
        a=b;
        b=temp%b;
        
    }
    return a;
}
bool prime(int a,int b)
{
    return gcd(a,b)==1;
}
void main()
{
    int n;
    printf("enter a number:");
    scanf("%d",&n);
    for(int i=5;i<=n;i++)
    {
        for(int j=4;j<i;j++)
        {
            for(int k=3;k<j;k++)
            {
                
                if(j*j+k*k==i*i)
                {
                    if(prime(i,j) && prime(j,k) && prime(i,k))
                    {
                        printf("%d %d %d\n",k,j,i);
                    }
                }
            }
        }
        
        
        
        
        
        
        
        
        
        
        
        
    }
}
